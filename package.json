{
  "name": "rspack-typewiz-plugin",
  "version": "2.0.0",
  "description": "Advanced runtime type collection & analysis system with SQLite storage and REST API for JavaScript code instrumentation",
  "main": "index.js",
  "private": false,
  "files": [
    "lib/",
    "examples/",
    "docs/",
    "index.js",
    "README.md",
    "TYPESCRIPT.md",
    "EXAMPLE.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "example": "cd examples && rspack serve --open",
    "build": "echo 'No build step required - JavaScript library'",
    "api-server": "node examples/start-api-server.js",
    "dev": "cd examples && concurrently \"rspack serve\" \"node start-api-server.js\"",
    "demo": "npm run dev",
    "lint": "eslint lib/ examples/ --fix",
    "typecheck": "echo 'No TypeScript files to check'",
    "package": "npm pack",
    "prepublishOnly": "npm run lint"
  },
  "keywords": [
    "typescript",
    "javascript",
    "type-inference",
    "runtime-analysis",
    "rspack",
    "webpack",
    "instrumentation",
    "sqlite",
    "api",
    "jsdoc",
    "code-analysis",
    "llm",
    "type-collection"
  ],
  "author": "TypeWiz Enhanced Contributors",
  "license": "ISC",
  "peerDependencies": {
    "@rspack/core": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@rspack/cli": "^1.0.0",
    "@rspack/core": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^9.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.4",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "style-loader": "^3.3.1",
    "supertest": "^7.1.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.8",
    "source-map": "^0.7.4"
  }
}
